{
    "name":"Checkbox marker",
    "description":"Check and uncheck checkboxes easily",
    "version":"1.0",
    "manifest_version":2,
    "content_scripts": 
    [
        {
            "matches":["<all_urls>"],
            "js":["jquery-3.3.1.min.js","checkboxes.js"]
        }
    ],
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_title": "Checkbox Marker",
        "default_popup": "popup.html"
      }
    }